<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4140.gr1811.app.ui.table.TableController">
   <children>
      <TableView fx:id="table" prefHeight="750.0" prefWidth="1040.0">
         <columns>
            <TableColumn fx:id="pnr" maxWidth="6000.0" minWidth="-1.0" prefWidth="5700.0" text="Personnummer" />
            <TableColumn fx:id="enavn" maxWidth="5500.0" minWidth="-1.0" prefWidth="5000.0" text="Etternavn" />
            <TableColumn fx:id="fnavn" minWidth="-1.0" prefWidth="5000.0" text="Fornavn" />
            <TableColumn fx:id="dato" maxWidth="5500.0" minWidth="-1.0" prefWidth="5500.0" text="Fødselsdato" />
            <TableColumn fx:id="k" maxWidth="3000.0" minWidth="-1.0" prefWidth="2500.0" text="Kjønn" />
            <TableColumn fx:id="h" maxWidth="4000.0" minWidth="-1.0" prefWidth="500.0" text="Høyde (cm)" />
            <TableColumn fx:id="v" maxWidth="4000.0" minWidth="-1.0" prefWidth="4000.0" text="Vekt (kg)" />
            <TableColumn fx:id="status" prefWidth="75.0" text="Status" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar prefWidth="200.0">
        <buttons>
          <Button fx:id="buttonSave" mnemonicParsing="false" text="Lagre til fil..." />
        </buttons>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
